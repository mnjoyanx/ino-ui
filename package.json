{
  "version": "1.5.11",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --format cjs,esm,umd",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "ino-ui-tv",
  "author": "andranikvn",
  "module": "dist/ino-ui-tv.esm.js",
  "size-limit": [
    {
      "path": "dist/ino-ui-tv.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/ino-ui-tv.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.6",
    "@types/nodemailer": "^6.4.16",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "husky": "^9.1.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^1.32.1",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.80.3",
    "size-limit": "^11.1.6",
    "tsdx": "^0.14.1",
    "tslib": "^2.8.0",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "@rollup/plugin-url": "^8.0.2",
    "mongodb": "^6.10.0",
    "nodemailer": "^6.9.16",
    "rollup-plugin-copy": "^3.5.0"
  }
}
